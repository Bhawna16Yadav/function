#include <stdio.h>
int circle(int r){
    int d=2*r;
    printf("diameter is:%d\n",d);
    int c=2*3.14*r;
    printf(circumference is:%d\n",c);
    int a=3.14*r*r;
    printf("area is:%d\n",a);
    }
    int main()
    {
        int r;
    printf("Enter radius of circle:\n");
    scanf("%d",&r);
   int k = circle(r);
   printf("%d",k);
    circle(r);
    return 0;
}
